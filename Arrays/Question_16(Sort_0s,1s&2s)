int findMissing(int arr[], int n) {
        // code here
           int a=arr[0];
           int d= (arr[n-1]-a)/n;
           int ap=a;
           for(int i=0;i<n;i++){
               if(ap!=arr[i]){
                   break;
               }
               ap=ap+d;
           }
           return ap;
           
           
    }
