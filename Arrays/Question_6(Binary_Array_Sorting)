    void binSort(int arr[], int n)
    {
       //Your code here
       
       /**************
        * No need to print the array
        * ************/
         int i=0,j=n-1;
         while(i<j){
             if(arr[i]==0){
                 i++;
             }
             else if(arr[i]==1){
                  swap(arr[i],arr[j]);
                  j--;
             }
                 
             }
         
        
    }
