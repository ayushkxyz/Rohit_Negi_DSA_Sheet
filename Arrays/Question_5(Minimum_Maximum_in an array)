pair<long long, long long> getMinMax(long long arr[], int n) {
           long long mini=INT_MAX;
           long long maxi=INT_MIN;
           for(int i=0;i<n;i++){
               maxi=max(arr[i],maxi);
               mini=min(arr[i],mini);
           }
           return {mini,maxi};
         
         
}
