 string merge (string s1, string s2)
{
    // your code here
    
     string s="";
     int i=0,j=0;
     
     while(i<s1.length() && j<s2.length()){
             s=s+s1[i];
             i++;
             
             
             s=s+s2[j];
             j++;
          
         
         
         
     }
     while(i<s1.length() ){
         s=s+s1[i];
         i++;
     }
     while(j<s2.length()){
         s=s+s2[j];
         j++;
     }
     return s;
}
